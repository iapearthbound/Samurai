#[mount]   [fs1]  [dev]                 [dev2]  [fs2]  [fsopts1]  [fsopts2]
/boot       bml    boot
/cache      ext4   /dev/block/stl11       NULL    rfs    NULL      rw,nosuid,nodev,check=no
/data       ext4   /dev/block/stl10       NULL    rfs    NULL      rw,nosuid,nodev,check=no
/recovery   bml    recovery
/sdcard     vfat   /dev/block/mmcblk0p1   NULL    NULL   NULL      NULL
/sd-ext     ext2   /dev/block/mmcblk0p2   NULL    NULL   NULL      NULL
/system     ext4   /dev/block/stl9        NULL    rfs    NULL      rw,check=no
